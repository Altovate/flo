<p id="notice"><%= notice %></p>

<h1>Listing Static Pages</h1>

<table>
  <thead>
    <tr>
      <th>Home</th>
      <th>Help</th>
      <th>Faqs</th>
      <th>Contact</th>
      <th>Terms</th>
      <th>Privacy</th>
      <th>About</th>
      <th>Pricing</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @static_pages.each do |static_page| %>
      <tr>
        <td><%= static_page.home %></td>
        <td><%= static_page.help %></td>
        <td><%= static_page.faqs %></td>
        <td><%= static_page.contact %></td>
        <td><%= static_page.terms %></td>
        <td><%= static_page.privacy %></td>
        <td><%= static_page.about %></td>
        <td><%= static_page.pricing %></td>
        <td><%= link_to 'Show', static_page %></td>
        <td><%= link_to 'Edit', edit_static_page_path(static_page) %></td>
        <td><%= link_to 'Destroy', static_page, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Static page', new_static_page_path %>
